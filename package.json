{
  "name": "flashlex-docs",
  "version": "1.0.0",
  "description": "docs for the flashlex project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "clean": "rm -rvf ./build",
    "build_docs": "$(npm bin)/antora site.yml",
    "build_swagger": "VERSION=$(bash iac/bash/version.sh) SWAGGER_FILE=$(bash iac/bash/swaggerfile.sh) && spectacle --target-dir build/site/flashlex-docs/$VERSION/swagger src/docs/swagger/$SWAGGER_FILE",
    "serve": "$(npm bin)/serve build/site",
    "buildui": "cd ui && $(npm bin)/gulp bundle && cd ..",
    "publish": "npm run clean && npm run build_swagger && npm run build_docs && bash iac/bash/copyweb.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@antora/cli": "^2.0.0",
    "@antora/site-generator-default": "^2.0.0",
    "serve": "^7.1.3",
    "swagger-node-codegen": "^1.6.3"
  },
  "devDependencies": {
    "asciidoctor.js": "1.5.9",
    "autoprefixer": "~9.4",
    "browserify": "~16.2",
    "cssnano": "~4.1",
    "eslint": "~5.11",
    "eslint-config-standard": "~12.0",
    "eslint-plugin-import": "~2.14",
    "eslint-plugin-node": "~8.0",
    "eslint-plugin-promise": "~4.0",
    "eslint-plugin-standard": "~4.0",
    "fs-extra": "~7.0",
    "gifsicle": "4.0.0",
    "gulp": "~4.0",
    "gulp-concat": "~2.6",
    "gulp-connect": "~5.7",
    "gulp-eslint": "~5.0",
    "gulp-imagemin": "~5.0",
    "gulp-postcss": "~8.0",
    "gulp-stylelint": "~8.0",
    "gulp-uglify": "~3.0",
    "gulp-vinyl-zip": "~2.1 >=2.1.2",
    "handlebars": "^4.1",
    "highlight.js": "~9.13",
    "js-yaml": "^3.13",
    "merge-stream": "~1.0",
    "postcss-calc": "~7.0",
    "postcss-custom-properties": "~8.0",
    "postcss-import": "~12.0",
    "postcss-url": "~8.0",
    "prettier-eslint": "~8.8",
    "require-directory": "~2.1",
    "require-from-string": "~2.0",
    "stylelint": "~9.9",
    "stylelint-config-standard": "~18.2",
    "through2": "~3.0",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "vinyl-buffer": "~1.0",
    "vinyl-fs": "~3.0"
  }
}
